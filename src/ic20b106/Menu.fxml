<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="ic20b106.MenuController" xmlns:fx="http://javafx.com/fxml"
      alignment="center" maxHeight="Infinity" maxWidth="Infinity">
    <Button fx:id="createGameButton" onMouseClicked="#createGame">
        Create New Game
    </Button>
    <Button fx:id="loadGameButton">
        Load Game
    </Button>
    <Button fx:id="optionsButton">
        Options
    </Button>
    <Button fx:id="exitButton">
        Exit Game
    </Button>
</VBox>